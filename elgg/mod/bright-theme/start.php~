<?php
elgg_register_event_handler('init', 'system', 'bright_theme_init');

function bright_theme_init() {
	elgg_unregister_menu_item('topbar', 'elgg_logo');
	
	elgg_register_menu_item('topbar', array(
			'href' => "#",
			'name' => 'account',
			//'priority' => 1000,
			'section' => 'alt',
			'text' => 'Options',
	));
}
?>
